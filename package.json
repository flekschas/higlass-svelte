{
  "name": "higlass-svelte",
  "version": "0.1.1",
  "description": "Svelte component for HiGlass",
  "author": [
    {
      "name": "Fritz Lekschas",
      "url": "https://lekschas.de"
    }
  ],
  "keywords": [
    "higlass",
    "svelte"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/higlass-svelte.git"
  },
  "files": [
    "higlass-svelte.js",
    "src/HiGlass.svelte"
  ],
  "main": "higlass-svelte.js",
  "module": "higlass-svelte.js",
  "svelte": "src/HiGlass.svelte",
  "scripts": {
    "build": "rollup -c component.rollup.config.js",
    "autobuild": "rollup -w -c demo.rollup.config.js",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "lint": "eslint src/* *.rollup.config.js",
    "prepublishOnly": "npm run lint",
    "prerelease": "npm run build; zip -j dist.zip higlass-svelte.js src/HiGlass.svelte"
  },
  "peerDependencies": {
    "svelte": "^3.24.0"
  },
  "dependencies": {
    "codemirror": "^5.55.0",
    "higlass": "^1.10.2",
    "lodash": "^4.17.19",
    "pixi.js": "^5.3.0",
    "react": "^16.6.3",
    "react-bootstrap": "0.32.1",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "page": "^1.11.4",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.18.2",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^6.1.0",
    "sirv-cli": "^1.0.3",
    "svelte": "^3.24.0"
  }
}
